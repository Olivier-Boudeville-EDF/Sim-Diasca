% This is a Sim-Diasca initialisation file for the Soda-test case.
%
% This example has been written by hand (not generated).
%


% It describes an exact replica of initial state as generated by
% soda_benchmarking_test.erl with no extra creation (i.e. with MachineCount and
% CustomerCount set to zero, simply obtained by using 'minimal' settings rather
% than 'normal' ones).


% Created on 2014/12/5 10:25:20 by Olivier Boudeville.



% Soda vending machine section.

% Construction parameters are: name, initial can count, can cost.


"First soda machine" <- { class_SodaVendingMachine, [ "First soda machine", 1000, 2.0 ] }.


"Second soda machine" <- { class_SodaVendingMachine, [ "Second soda machine", 8, 1.5 ] }.


"Third soda machine" <- { class_SodaVendingMachine, [ "Third soda machine", 8500, 1.7 ] }.


"Fourth soda machine" <- { class_SodaVendingMachine, [ "Fourth soda machine", 8500, 1.7 ] }.


"Fifth soda machine" <- { class_SodaVendingMachine, [ "Fifth soda machine", 5500, 1.8 ] }.


"Sixth soda machine" <- { class_SodaVendingMachine, [ "Sixth soda machine", 8500, 1.7 ] }.


"Seventh soda machine" <- { class_SodaVendingMachine, [ "Seventh soda machine", 8500, 1.7 ] }.




% Deterministic customer section.

% Construction parameters are: name, known machine, repletion duration, initial
% budget.


"John" <- { class_DeterministicThirstyCustomer, [ "John", { user_id, "First soda machine" }, 1, 16.0 ] }.


"Terry" <- { class_DeterministicThirstyCustomer, [ "Terry", { user_id, "First soda machine" }, 3, 800.0 ] }.


"Michael" <- { class_DeterministicThirstyCustomer, [ "Michael", { user_id, "Second soda machine" }, 2, 7000.0 ] }.


"Jordan" <- { class_DeterministicThirstyCustomer, [ "Jordan", { user_id, "Third soda machine" }, 2, 7000.0 ] }.


"Johnson" <- { class_DeterministicThirstyCustomer, [ "Johnson", { user_id, "Third soda machine" }, 2, 7000.0 ] }.


"Nash" <- { class_DeterministicThirstyCustomer, [ "Nash", { user_id, "Fourth soda machine" }, 2, 7000.0 ] }.


"Bird" <- { class_DeterministicThirstyCustomer, [ "Bird", { user_id, "Fifth soda machine" }, 2, 7000.0 ] }.


"Durant" <- { class_DeterministicThirstyCustomer, [ "Durant", { user_id, "Seventh soda machine" }, 2, 7000.0 ] }.





% Stochastic customer section.

% Construction parameters are: name, known machine, repletion duration law,
% initial budget.


"Arnold"  <- { class_StochasticThirstyCustomer, [ "Arnold", { user_id, "Second soda machine" }, { gaussian, 10, 2 }, 1800.0 ] }.


"Willy"  <- { class_StochasticThirstyCustomer, [ "Arnold", { user_id, "Third soda machine" }, { uniform, 15 }, 1200.0 ] }.



"Thelma" <- { class_StochasticThirstyCustomer, [ "Thelma", { user_id, "Third soda machine" }, { uniform, 22 }, 1200.0 ] }.


"Louise" <- { class_StochasticThirstyCustomer, [ "Louise", { user_id, "Third soda machine" }, { uniform, 155 }, 1200.0 ] }.



"Homer" <- { class_StochasticThirstyCustomer, [ "Homer", { user_id, "Third soda machine" }, { uniform, 5 }, 1200.0 ] }.


"Marge" <- { class_StochasticThirstyCustomer, [ "Marge", { user_id, "Third soda machine" }, { uniform, 189 }, 1200.0 ] }.


"Bart" <- { class_StochasticThirstyCustomer, [ "Bart", { user_id, "First soda machine" }, { uniform, 122 }, 1200.0 ] }.


"Lisa" <- { class_StochasticThirstyCustomer, [ "Lisa", { user_id, "Sixth soda machine" }, { uniform, 88 }, 1200.0 ] }.


"Maggie" <- { class_StochasticThirstyCustomer, [ "Maggie", { user_id, "Second soda machine" }, { uniform, 25 }, 1200.0 ] }.
